    <!-- actions opening views on models -->
    <record id="view_sale_order_form" model="ir.ui.view">
      <field name="name">view.sale.order.form</field>
      <field name="model">sale.order</field>
       <field name="inherit_id" ref="sale.view_order_form" />
      <field name="arch" type="xml">
          
          <field name="partner_id" 
          position="after">
          
          <field name="restaurant_id" readonly="0"/>
          
          
          </field>
          
      </field>
    </record>

    <record id="view_restaurant_profile" model="ir.ui.view">
        <field name="name">view.res.company.form</field>
        <field name="model">res.company</field>
         <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            
            <field name="name"
            position="after">
            
            <div name="buttons" class="oe_right oe_button_box">
              <button id="button_id" 
                name="%(product.product_template_action)d"
                type="action"
                class="oe_stat_button"
                icon="fa-tasks"
                help="view stock info">
                <!-- <field name="stock" string="Stock Value" widget="statinfo"/> -->
              </button>
            </div>
            
            </field>
            
        </field>
    </record>

    
    
    

  
    
   
